<section>
  <h2>List of Users</h2>
  <button type="button" class="new-post-btn">
    <%= link_to "Create A Post", new_post_path(@users.first.id) , class: "new-post-link" %>
  </button>
  <ul class="users">
    <%@users.each do |user|%>
      <li class="users-list">
        <a href='/users/<%= user.id %>'>
          <%= render 'shared/user', user:user %>
        </a>
      </li>
    <%end%>
  </ul>
</section>